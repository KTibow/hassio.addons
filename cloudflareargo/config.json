{
	"name": "Cloudflare Argo",
	"version": "0.3",
	"slug": "argocloudflare",
	"description": "Argo for Home Assistant",
	"url": "https://github.com/wlatic/hassio.addons/blob/master/cloudflareargo/readme.md",
	"ingress": false,
	"panel_icon": "mdi:file-document-box-outline",
	"homeassistant": "0.101.0b0",
	"arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
	"map": [
         "config:rw"
  ],

	"startup": "services",
	"boot": "auto",
	"options": {
		
			"hostname": "hostname",
			"haurl": "http://hassio:8123",
			"certificate": "/config/cert/argo.pem"
	},
	"schema": false
}
